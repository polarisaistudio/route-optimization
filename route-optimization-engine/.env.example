# Application
NODE_ENV=development
PORT=3001
API_BASE_URL=http://localhost:3001

# MongoDB
MONGODB_URI=mongodb://localhost:27017/route_optimization
MONGODB_DB_NAME=route_optimization

# Redis
REDIS_URL=redis://localhost:6379

# Snowflake
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_user
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_DATABASE=FIELD_SERVICE_OPS
SNOWFLAKE_SCHEMA=ANALYTICS
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_ROLE=ANALYST_ROLE

# AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_key
AWS_SECRET_ACCESS_KEY=your_secret
S3_BUCKET_NAME=route-optimization-data
LAMBDA_FUNCTION_NAME=route-optimizer

# Palantir Foundry
FOUNDRY_URL=https://your-instance.palantirfoundry.com
FOUNDRY_TOKEN=your_token

# Salesforce
SF_USERNAME=your_sf_user
SF_PASSWORD=your_sf_password
SF_SECURITY_TOKEN=your_sf_token
SF_DOMAIN=login

# Okta SSO
OKTA_ISSUER=https://your-domain.okta.com/oauth2/default
OKTA_CLIENT_ID=your_client_id
OKTA_CLIENT_SECRET=your_client_secret
OKTA_AUDIENCE=api://route-optimization

# New Relic
NEW_RELIC_ENABLED=false
NEW_RELIC_LICENSE_KEY=your_license_key
NEW_RELIC_APP_NAME=Route Optimization Engine

# Optimization
MAX_OPTIMIZATION_TIME_SECONDS=300
DEFAULT_ALGORITHM=vrp
MAX_TECHNICIANS_PER_RUN=50

# JWT
JWT_SECRET=your-jwt-secret-change-in-production
JWT_EXPIRY=8h
